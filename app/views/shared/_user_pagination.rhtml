<aside class="pagination">
  <% @types.each do |type| -%>
	<div class="all_<%= type.to_s %>_pagination" data-type="<%= type.to_s %>">
	<%= will_paginate @results[type], :container => false, :previous_label => '&laquo;', :next_label => '&raquo;', :params => { :ajax_region => "#{type.to_s}" } %>
	</div>
  <% end -%>
</aside>
