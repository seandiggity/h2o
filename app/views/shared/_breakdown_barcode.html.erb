<div class="barcode_breakdown">
  <% model::RATINGS_DISPLAY.each do |k, v| -%>
  <% if breakdown_data.has_key?(k) -%>
  <div class="<%= k %>">
    <span class="icon icon-<%= k %>"></span>
    <span class="detail">
      <b><%= v %></b> <%= pluralize breakdown_data[k], 'time' %>
    </span>
  </div>
  <% end -%>
  <% end -%>
</div>
